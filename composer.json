{
    "name": "benagricola/eks-auth",
    "type": "library",
    "keywords": ["kubernetes","aws","eks","aws-eks"],
    "homepage": "https://github.com/benagricola/php-eks-auth",
    "license": "MIT",
    "description": "Provides a pre-authenticated HTTPClient for consumption by existing PHP Kubernetes Client libraries",
    "suggest": {
        "maclof/kubernetes-client": "^0.21.0",
        "flow/jsonpath": "^0.5.0"
    },
    "require": {
        "php": ">=7.3",
        "aws/aws-sdk-php": "^3.179",
        "guzzlehttp/psr7": "^1.8"
    },
    "autoload": {
        "psr-4": {"EKSAuth\\": "src/"}
    },
    "require-dev": {
        "overtrue/phplint": "^2.3"
    }
}
